<h1>Results</h1>

<!-- TABLE (HEAD/BODY)
------------------------------>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Campaign ID</th>
      <th>Campaign name</th>
      <th>Cuepoint ID</th>
      <th>Cuepoint</th>
      <th>Start</th>
      <th>Complete</th>
    </tr>
  </thead>
  <tbody>
    <% @results.each do |result| %>
    <tr>
      <td><%= link_to result.campaign.id %></td>
      <td><%= link_to result.campaign.name %></td>
      <td><%= link_to result.cuepoint.id %></td>
      <td><%= link_to result.cuepoint.name %></td>
      <td><%= link_to result.count_start %></td>
      <td><%= link_to result.count_complete %></td>
    </tr>
    <% end %>
  </tbody>
</table>