<h1>キューポイント登録</h1>

<!-- FORM (from partial)
------------------------------>
<%= form_for(@cuepoint) do |f| %>

<%= render 'shared/error_messages',  object: f.object %>
<%= render partial: 'cuepoints/form', locals: { f: f } %>

<!-- BUTTONS
------------------------------>
<%= f.submit '登録', class: "btn btn-success" %>
<%= link_to '中止', cuepoints_path, class: "btn btn-default" %>

<% end %>
