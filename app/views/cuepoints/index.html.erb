<h1>キューポイント一覧</h1>

<!-- REGISTER BUTTON
------------------------------>
<%= link_to "登録", new_cuepoint_path, class: "btn btn-default" %>

<!-- TABLE (HEAD/BODY)
------------------------------>
<table class="table table-hover">
  <thead>
    <tr>
      <th>id</th>
      <th>cue point</th>
      <th>create</th>
      <th>update</th>
    </tr>
  </tdead>
  <tbody>
    <% @cuepoints.each do |cuepoint| %>
      <tr>
        <td><%= link_to cuepoint.id, edit_cuepoint_path(cuepoint) %></td>
        <td><%= cuepoint.name %></td>
        <td><%= cuepoint.created_at %></td>
        <td><%= cuepoint.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
  
